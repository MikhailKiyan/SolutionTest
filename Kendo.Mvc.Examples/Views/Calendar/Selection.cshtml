<div class="demo-section k-content" style="text-align: center;">
    <h4>Multiple Date Selection</h4>
    @(Html.Kendo().Calendar()
            .Name("calendar")
            .WeekNumber(true)
            .Selectable("multiple")
            .DisableDates(DayOfWeek.Saturday, DayOfWeek.Sunday)
    )
</div>
